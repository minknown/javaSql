<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mayizt.mapper.JmMapper">
    <select id="selectAll" resultType="com.mayizt.pojo.Jm">
        select * from jm limit 3
    </select>
    <select id="selectByid" resultType="Jm">
        select * from user where id=#{id}
    </select>
</mapper>